    "            system(\"perl -pi -e 's/,/./g' %s \" % path)\n",
    "\n",
    "        else:\n",
    "            raise RuntimeError(\"Information provided insufficient\")\n",
    "\n",
    "    def __str__(self):\n",
    "        s = \"'\" + self.__class__.__name__ + \"' object at '\" + self.path + \"'.\\n\"\n",
